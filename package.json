  },
  "scripts": {
    "dev": "next dev",
    "debug": "NODE_OPTIONS=\"--inspect\" next dev -H 0.0.0.0",
    "build": "next build",
    "test": "jest",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:e2e": "npx playwright test",
    "test:e2e:debug": "PWDEBUG=1 npx playwright test",
    "test:e2e:watch": "nodemon -e ts,tsx --watch specs --watch lib --watch pages --exec npx playwright test",
    "test:hook": "jest --bail --passWithNoTests",
